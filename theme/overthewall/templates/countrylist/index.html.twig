{% extends 'page.html.twig' %}

{% block content %}

	<h2>Countries</h2>

	<ul class="mainListCountries">
		{%  for country in countries %}
			<li class="country">
				<div class="title"><a href="{{ ("/country/" ~ country.code|upper) | internalLinkToDir }}">{{  country.title }}</a></div>
			</li>
		{% endfor %}
	</ul>

{% endblock %}
