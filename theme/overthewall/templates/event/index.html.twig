{% extends 'page.html.twig' %}

{% block content %}

	<p>EVENT</p>

	<ul>
		<li>
			<div class="">{{  event.title }}</div>
			<div class="">{{  event.start | date("F jS \\a\\t g:ia") }}</div>
			<div class="">{{  event.end | date("F jS \\a\\t g:ia") }}</div>
			<div class="">{{  event.country }}</div>
			<div class="">{{  event.timeZone }}</div>
		</li>
	</ul>

{% endblock %}